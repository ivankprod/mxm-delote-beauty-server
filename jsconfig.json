{
    "compilerOptions": {
        "baseUrl": "./src",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2020",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true
    },
	"paths": {
		"#core/*": ["./*"]
	},
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}
